<main>
<head>
	<meta name='viewport' content='width=device-width, initial-scale=1'>
	<script src="https://kit.fontawesome.com/2dc22d6664.js" crossorigin='anonymous'></script>
<link rel="preconnect" href="https://fonts.googleapis.com">
<link rel="preconnect" href="https://fonts.gstatic.com" crossorigin>
<link href="https://fonts.googleapis.com/css2?family=Poppins&display=swap" rel="stylesheet">
<link rel="stylesheet" href="../../assets/css/facultystyles.css">
</head>
<style>
.mydiv2{
  border: 0px outset black ;
  height:27%;
  width:95%;
  border-radius: 6px;
  background-color: #FFFFFF ;
  position: absolute;
  left: 2%;
  top:  20%;
  margin:0px;
  padding:0px;
  color: black;
}
</style>
</head>
<a href="/faculty/dashboard"> 
	<i class='fas fa-arrow-alt-circle-left' style='font-size:24px;color:#707070;'></i>
	
</a>
<h1 style="color: black;">Previous Tickets</h1>
<div class="mydiv2">
<table  width=100% height=100% bgcolor=#FFFFFF align="center" >
	<thead>
		<tr style="color: black;">
			  <th scope="col">Sl No.</th>
			  <th scope="col">Building No.</th>
			  <th scope="col">Room No.</th>
			  <th scope="col">Nature</th>
			  <th scope="col">Problem</th>
			  <th scope="col">Staff</th>
			  <th scope="col">Actions</th>
		 </tr>
	</thead>
				<tbody style="color: black;">
					<% for(let i=0; i<previousTickets.length; i++) { %>
						<tr>
							<th scope="row"> <%= i+1 %> </th>
							<td> <%= previousTickets[i].building%> </td>
							<td> <%= previousTickets[i].room %> </td>
							<td> <%= previousTickets[i].nature %> </td>
							<td> <%= previousTickets[i].problem %> </td>
							<td> <%= previousTickets[i].staff.firstName + " " + previousTickets[i].staff.lastName %> </td>
							<td class="fw-bold text-<%= previousTickets[i].status %>"> <%= previousTickets[i].status %> </td>
						</tr>
					<% } %>
				</tbody>
			</table>
		</div>
		
	</div>
</main>